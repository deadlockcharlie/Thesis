  \entry{felber_hardware_2016}{inproceedings}{}
    \name{author}{4}{}{%
      {{hash=FP}{%
         family={Felber},
         familyi={F\bibinitperiod},
         given={Pascal},
         giveni={P\bibinitperiod},
      }}%
      {{hash=IS}{%
